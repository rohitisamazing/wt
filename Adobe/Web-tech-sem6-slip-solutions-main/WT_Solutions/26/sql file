CREATE TABLE EMP (
    eno INT PRIMARY KEY,
    ename VARCHAR(255),
    designation VARCHAR(255),
    salary DECIMAL(10, 2)
);


INSERT INTO EMP (eno, ename, designation, salary) VALUES
(1, 'John Doe', 'Manager', 50000),
(2, 'Jane Smith', 'Developer', 60000),
(3, 'Bob Johnson', 'Analyst', 55000);
